{
  "name": "zenml-vscode-quickstart",
  "displayName": "ZenML Quickstart",
  "description": "",
  "version": "0.0.1",
  "publisher": "ZenML",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zenml-walkthrough",
          "title": "ZenMl Walkthrough",
          "icon": "$(book)"
        }
      ]
    },
    "views": {
      "zenml-walkthrough": [
        {
          "type": "webview",
          "id": "zenml.stepsView",
          "name": "ZenMl"
        }
      ]
    },
    "commands": [
      {
        "command": "zenml.openDocPanel",
        "title": "Opens markdown file",
        "category": "ZenMl"
      },
      {
        "command": "zenml.openCodePanel",
        "title": "Opens tutorial code file",
        "category": "ZenMl"
      },
      {
        "command": "zenml.runCurrentPythonFile",
        "title": "Runs the currently visible py file",
        "category": "ZenMl"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "marked": "^13.0.1"
  }
}
